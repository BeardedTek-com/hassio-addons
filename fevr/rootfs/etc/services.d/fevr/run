#!/usr/bin/with-contenv bashio
# ==============================================================================
# Start the example service
# s6-overlay docs: https://github.com/just-containers/s6-overlay
# ==============================================================================

# Add your code here

# Declare variables
declare debug
declare title
declare frigate
## Get the 'message' key from the user config options.
debug=$(bashio::config 'debug')
title=$(bashio::config 'title')
frigate=$(bashio::config 'frigate')


## Run your program
exec /writeConfig ${debug} ${title} ${frigate} && \ # Launch python script to populate config.json file
     httpd && \ # start apache2
     sqlite_web -p 8002 -H 0.0.0.0 -x /var/www/db/fEVR.sqlite # Start sqlite_web